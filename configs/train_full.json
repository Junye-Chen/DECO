{
    "seed": 2024,
    "save_dir": "experiments_model/",
    "train_data_loader": {
        "name": "youtube-vos",
        "video_root": "./youtube-vos/JPEGImages/",
        "wm_root": "./wm/",
        "w": 432,
        "h": 240,
        "num_local_frames": 10,
        "num_ref_frames": 4,
        "load_flow": 0,
        "is_train": 0
    },
    "losses": {
        "hole_weight": 1,
        "valid_weight": 1,
        "flow_weight": 1,
        "adversarial_weight": 0.01,
        "GAN_LOSS": "hinge",
        "perceptual_weight": 0.25,
        "selfperceptual_weight": 0
    },
    "model": {
        "net": "msvt",
        "no_dis": 0,
        "load_d": 1,
        "interp_mode": "nearest",
        "pretrained": "nearest"
    },
    "trainer": {
        "version": "trainer_full",
        "type": "Adam",
        "beta1": 0,
        "beta2": 0.99,
        "glr": 1e-4,
        "dlr": 1.5e-4,
        "batch_size": 4,
        "num_workers": 8,
        "num_prefetch_queue": 8,
        "log_freq": 100,
        "save_freq": 10e3,
        "iterations": 80e4,
        "scheduler": {
            "type": "MultiStepLR",
            "milestones": [
                40000e3
            ],
            "gamma": 0.1
        }
    }
}
